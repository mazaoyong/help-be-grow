{
  "project": {
    "projectName": "wsc-pc-vis"
  },
  "hash": [
    {
      "src":"../static/build/h5/**/*.js",
      "dest": "../config/version_h5_js.json",
      "content": {
        "recordTpl": "\"<%= name %>\" : \"wsc-pc-vis/h5/<%= path %>.<%= extension %>\""
      }
    },
    {
      "src": "../static/build/h5/**/*.css",
      "dest": "../config/version_h5_css.json",
      "content": {
        "recordTpl": "\"<%= name %>\" : \"wsc-pc-vis/h5/<%= path %>.<%= extension %>\""
      }
    }
  ],
  "cdn": {
    "basic": {
      "src": ["../static/build/h5/**/*.js", "../static/build/h5/**/*.css"],
      "dist": "/wsc-pc-vis/h5"
    }
  },
  "build": {
    "common": {
      "babel": {
        "test": "/\\.(js|jsx)$/"
      },
      "vue": {
        "enable": true
      },
      "entry": [
        {
          "path": "pages",
          "recursive": true
        },
        {
          "path": "pages",
          "recursive": true,
          "index": "main.ts"
        }
      ],
      "dll": {
        "name": "dll",
        "path": "./common/dll.js"
      },
      "common": {
        "name": "base",
        "path": "./common/base.js"
      },
      "style": {
        "test": "/\\.(s?css|pcss|postcss)$/",
        "options": {
          "extract": true,
          "style-loader": true,
          "css-loader": {
            "options": {
              "importLoaders": 2
            }
          },
          "postcss-loader": {
            "options": {
              "includePaths": ["styles", "node_modules"],
              "autoprefixer": true,
              "sass-like": {
                "unresolved": "ignore"
              }
            }
          }
        }
      },
      "configPath": "./webpack.conf.js",
      "react": false
    },
    "dev": {
      "output": {
        "js": "../static/local/h5",
        "css": "../static/local/h5"
      },
      "browsers": ["Android >= 4.0", "iOS >= 7"]
    },
    "prd": {
      "output": {
        "js": "../static/build/h5",
        "css": "../static/build/h5"
      },
      "browsers": ["Android >= 4.0", "iOS >= 7"]
    }
  }
}
